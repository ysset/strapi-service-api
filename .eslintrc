{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "globals": {
    "afterAll": "readonly",
    "afterEach": "readonly",
    "beforeAll": "readonly",
    "beforeEach": "readonly",
    "strapi": "readonly",
    "describe": "readonly",
    "it": "readonly",
    "expect": "readonly",
    "jest": "readonly"
  },
  "rules": {
    "quotes": ["warn", "single"],
    "semi": ["off"],
    "no-unused-vars": "warn"
  }
}
